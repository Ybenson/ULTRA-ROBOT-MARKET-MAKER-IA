# Exemple de fichier .env pour ULTRA-ROBOT MARKET MAKER IA
# Copiez ce fichier vers .env et remplissez avec vos propres clés API

# Binance API (Crypto)
BINANCE_API_KEY=votre_clé_api_binance
BINANCE_API_SECRET=votre_secret_api_binance

# Alpaca API (Actions)
ALPACA_API_KEY=votre_clé_api_alpaca
ALPACA_API_SECRET=votre_secret_api_alpaca

# Oanda API (Forex)
OANDA_API_KEY=votre_clé_api_oanda
OANDA_ACCOUNT_ID=votre_id_compte_oanda

# Paramètres de notification
TELEGRAM_BOT_TOKEN=votre_token_bot_telegram
TELEGRAM_CHAT_ID=votre_chat_id_telegram
EMAIL_SMTP_SERVER=smtp.example.com
EMAIL_SMTP_PORT=587
EMAIL_USERNAME=votre_email@example.com
EMAIL_PASSWORD=votre_mot_de_passe_email

# Paramètres généraux
ULTRA_BOT_GENERAL_MODE=simulation
ULTRA_BOT_RISK_MANAGEMENT_MAX_POSITION_SIZE=2000
ULTRA_BOT_RISK_MANAGEMENT_MAX_DRAWDOWN_PERCENT=3.0

# Paramètres de base de données
DB_HOST=localhost
DB_PORT=5432
DB_NAME=ultra_bot
DB_USER=postgres
DB_PASSWORD=votre_mot_de_passe_db

# Redis (pour le cache haute vitesse)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=votre_mot_de_passe_redis

# ClickHouse (pour les données de séries temporelles)
CLICKHOUSE_HOST=localhost
CLICKHOUSE_PORT=9000
CLICKHOUSE_USER=default
CLICKHOUSE_PASSWORD=votre_mot_de_passe_clickhouse
